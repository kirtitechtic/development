            {%- if section.settings.title_font_size == 'custom' -%}
            #woodmart-text-block-id2-{{section.id}} .woodmart-text-block{font-size:{{section.settings.title_font_size_custom | remove: 'px' | append: 'px'}};line-height:{{section.settings.title_font_size_custom | remove: 'px' | plus: 10 | append: 'px'}};}
