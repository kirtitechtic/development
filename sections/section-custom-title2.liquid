            <div class="woodmart-title-container woodmart-text-block font-{{section.settings.sub_title_font}} woodmart-font-weight-{{section.settings.sub_title_font_weight}}">
              {%- if settings.language_enable -%}
              <span class="lang1">{{ section.settings.sub_title | split: '|' | first }}</span>
              <span class="lang2">{{ section.settings.sub_title | split: '|' | last }}</span>
              {%- else -%}
              {{ section.settings.sub_title | split: '|' | first }}
              {%- endif -%}
            </div>
          </div>
          {%- endif -%}
          {%- if section.settings.title != '' -%}
