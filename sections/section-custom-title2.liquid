{%- case section.settings.section_layout -%}
{%- when '0' -%}
{%- capture data_fullwidth -%}{%- endcapture -%}
{%- when '1' -%}
{%- capture data_fullwidth -%}data-vc-full-width="true" data-vc-full-width-init="true"{%- endcapture -%}
{%- when '2' -%}
{%- capture data_fullwidth -%}data-vc-full-width="true" data-vc-full-width-init="true" data-vc-stretch-content="true"{%- endcapture -%}
{%- when '3' -%}
{%- capture data_fullwidth -%}data-vc-full-width="true" data-vc-full-width-init="true" data-vc-stretch-content="true"{%- endcapture -%}
{%- endcase -%} 
<div data-section-id="{{ section.id }}" data-section-type="carousel-section">
  <div {{data_fullwidth}} class="vc_row wpb_row vc_row-fluid vc_custom_{{section.id}} vc_row-has-fill woodmart-bg-{{section.settings.background_position | replace: '_', '-' }} {% if section.settings.section_layout == '3' %}vc_row-no-padding{% endif %} woodmart-bg-left-top" style="margin-top:{{section.settings.margin_top | remove: 'px' | append: 'px' }}; margin-bottom:{{section.settings.margin_bottom | remove: 'px' | append: 'px' }};padding: {{section.settings.padding}};">
    <div class="wpb_column vc_column_container vc_col-sm-12">
      <div class="vc_column-inner">
        <div class="wpb_wrapper">
          {%- if section.settings.sub_title != '' -%}
          <style>
            {%- if section.settings.sub_title_font_size == 'custom' -%}
            #woodmart-text-block-id1-{{section.id}} .woodmart-text-block{font-size:{{section.settings.sub_title_font_size_custom | remove: 'px' | append: 'px'}};line-height:{{section.settings.sub_title_font_size_custom | remove: 'px' | plus: 10 | append: 'px'}};}
            {%- if section.settings.sub_title_font_size_custom_tablet != '' -%}
            @media (max-width: 1024px){#woodmart-text-block-id1-{{section.id}} .woodmart-text-block{font-size:{{section.settings.sub_title_font_size_custom_tablet | remove: 'px' | append: 'px'}};line-height:{{section.settings.sub_title_font_size_custom_tablet | remove: 'px' | plus: 10 | append: 'px'}};}}
            {%- endif -%}
            {%- if section.settings.sub_title_font_size_custom_mobile != '' -%}
            @media (max-width: 767px){#woodmart-text-block-id1-{{section.id}} .woodmart-text-block{font-size:{{section.settings.sub_title_font_size_custom_mobile | remove: 'px' | append: 'px'}};line-height:{{section.settings.sub_title_font_size_custom_mobile | remove: 'px' | plus: 10 | append: 'px'}};}}
            {%- endif -%}
            {%- endif -%}
            {%- if section.settings.sub_title_color == 'custom' -%}
            #woodmart-text-block-id1-{{section.id}} .woodmart-text-block{color:{{section.settings.sub_title_custom_color}};}
            {%- endif -%}
            .vc_custom_{{section.id}}1 {
              {%- if section.settings.sub_title_margin_top != '' -%}
              margin-top: {{section.settings.sub_title_margin_top | remove: 'px' | append: 'px'}};
              {%- endif -%}
              {%- if section.settings.sub_title_margin_right != '' -%}
              margin-right: {{section.settings.sub_title_margin_right | remove: 'px' | append: 'px'}};
              {%- endif -%}
              {%- if section.settings.sub_title_margin_bottom != '' -%}
              margin-bottom: {{section.settings.sub_title_margin_bottom | remove: 'px' | append: 'px'}};
              {%- endif -%}
              {%- if section.settings.sub_title_margin_left != '' -%}
              margin-left: {{section.settings.sub_title_margin_left | remove: 'px' | append: 'px'}};
              {%- endif -%}
