{%- case section.settings.section_layout -%}
{%- when '0' -%}
{%- capture data_fullwidth -%}{%- endcapture -%}
{%- when '1' -%}
{%- capture data_fullwidth -%}data-vc-full-width="true" data-vc-full-width-init="true"{%- endcapture -%}
{%- when '2' -%}
{%- capture data_fullwidth -%}data-vc-full-width="true" data-vc-full-width-init="true" data-vc-stretch-content="true"{%- endcapture -%}
{%- when '3' -%}
{%- capture data_fullwidth -%}data-vc-full-width="true" data-vc-full-width-init="true" data-vc-stretch-content="true"{%- endcapture -%}
{%- endcase -%} 
<div data-section-id="{{ section.id }}" data-section-type="promo-banner-section">
  <div {{data_fullwidth}} class="vc_row wpb_row vc_row-fluid {% if section.settings.hide_background_mobile != blank %}mobile-bg-img-hidden{% endif %} vc_custom_{{section.id}} {% if section.settings.section_layout == '3' %}vc_row-no-padding{% endif %}"  style="margin-top: {{section.settings.margin_top | remove: 'px' | append: 'px' }};margin-bottom: {{section.settings.margin_bottom | remove: 'px' | append: 'px' }};padding: {{section.settings.padding}};">
    {{section.settings.background_gradient_color}}
    <div class="wpb_column vc_column_container vc_col-sm-12">
      <div class="vc_column-inner">
        <div class="wpb_wrapper">
          <div class="title-wrapper woodmart-title-color-{{section.settings.heading_color}} woodmart-title-style-{{section.settings.heading_style}} woodmart-title-size-{{section.settings.heading_size}} woodmart-title-width-{{section.settings.heading_width}} text-{{section.settings.heading_postion}}">
            {%- if section.settings.heading_top != '' -%}
            <div class="title-subtitle font-default style-{{section.settings.heading_style}}">
              {%- if settings.language_enable -%}
              <span class="lang1">{{ section.settings.heading_top | split: '|' | first }}</span>
              <span class="lang2">{{ section.settings.heading_top | split: '|' | last }}</span>
              {%- else -%}
              {{ section.settings.heading_top | split: '|' | first }}
              {%- endif -%}
            </div>
            {%- endif -%} 
            {%- if section.settings.title != '' -%}
            <div class="liner-continer">
              <span class="left-line"></span>
              <h4 class="woodmart-title-container title  woodmart-font-weight-">
                {%- if settings.language_enable -%}
                <span class="lang1">{{ section.settings.title | split: '|' | first }}</span>
                <span class="lang2">{{ section.settings.title | split: '|' | last }}</span>
                {%- else -%}
                {{ section.settings.title | split: '|' | first }}
                {%- endif -%}
              </h4>
              <span class="right-line"></span>
            </div>
            {%- endif -%}
            {%- if section.settings.heading_content != '' -%}
            <div class="title-after_title">
              {%- if settings.language_enable -%}
              <span class="lang1">{{ section.settings.heading_content | split: '|' | first }}</span>
              <span class="lang2">{{ section.settings.heading_content | split: '|' | last }}</span>
              {%- else -%}
              {{ section.settings.heading_content | split: '|' | first }}
              {%- endif -%}
            </div>
            {%- endif -%}
            <style> 
              {%- if section.settings.heading_top == '' and section.settings.title == '' and section.settings.heading_content == '' -%}
              .vc_custom_{{section.id}} .title-wrapper {
                margin-bottom: 0;
              }
              {%- endif -%} 
            </style>
          </div>
          {%- if section.blocks.size > 0 -%} 
            <div class="vc_row wpb_row vc_row-fluid vc_row-o-equal-height vc_row-o-content-middle vc_row-flex woodmart-row-gradient-enable">
              {%- for block in section.blocks -%}
              {%- case block.type -%}
              {%- when 'banner' -%}
              <div class="{% if block.settings.item_hidden_tablet != blank %}tablet-bg-img-hidden{% endif %}{% if block.settings.item_hidden_mobile != blank %} mobile-bg-img-hidden{% endif %} wpb_column vc_column_container vc_col-sm-{{block.settings.item_width_mobile}} vc_col-lg-{{block.settings.item_width_desktop}} vc_col-md-{{block.settings.item_width_tablet}} vc_col-has-fill{%if block.settings.box_background_position != 'default' %} woodmart-bg-{{block.settings.box_background_position | replace: '_', ' ' }}{% endif %}">
                <div class="vc_column-inner vc_box_custom_{{ block.id }}">
                  <div class="hapiness"> </div>
                  <div class="wpb_wrapper"> 
                    <div class=" woodmart-info-box text-{{block.settings.content_text_align}} box-title-{{block.settings.title_size}} icon-alignment-{{block.settings.icon_position}} color-scheme-{{block.settings.content_color}} with-btn btn-position-{{block.settings.button_position}} vc_content_custom_{{block.id}}">
                      {%- if block.settings.icon_use != blank  -%}
                      <div class="box-icon-wrapper  with-icon icon-style-simple">
                        <div class="info-box-icon">
                          {%- if block.settings.icon_code != '' -%}
                            {{block.settings.icon_code}}
                          {%- else -%}
                          	{%- if block.settings.icon_type == 'text' -%}
                              <i class="{{block.settings.icon_text}}"></i>
                            {%- else-%}
                          	  <img src="{{block.settings.icon_image | img_url: 'master'}}" />
                          	{%- endif -%}
                          {%- endif -%}
                        </div>
                      </div>
                      {%- endif -%}
                      <div class="info-box-content">
                        {%- if block.settings.title != '' -%}
                        <h4 class="info-box-title">
                          {%- if settings.language_enable -%}
                          <span class="lang1">{{ block.settings.title | split: '|' | first }}</span>
                          <span class="lang2">{{ block.settings.title | split: '|' | last }}</span>
                          {%- else -%}
                          {{ block.settings.title | split: '|' | first }}
                          {%- endif -%}
                        </h4>
                        {%- endif -%}
                        {%- if block.settings.content != '' -%}
                        <div class="info-box-inner">
                          {%- if settings.language_enable -%}
                          <div class="lang1">{{ block.settings.content | split: '[lang2]' | first }}</div>
                          <div class="lang2">{{ block.settings.content | split: '[lang2]' | last }}</div>
                          {%- else -%}
                          {{ block.settings.content | split: '[lang2]' | first }}
                          {%- endif -%}
                        </div>
                        {%- endif -%}
                        {%- if block.settings.button_title != '' -%}
                        <div class="info-btn-wrapper">
                          <div class="woodmart-button-wrapper text-{{block.settings.content_text_align}}">
                            <a class="btn btn-color-{{block.settings.button_color}} btn-scheme-light btn-scheme-hover-light btn-style-{{block.settings.button_style}} btn-shape-rectangle btn-size-{{block.settings.button_size }}">
                              {%- if settings.language_enable -%}
                              <span class="lang1">{{ block.settings.button_title | split: '|' | first }}</span>
                              <span class="lang2">{{ block.settings.button_title | split: '|' | last }}</span>
                              {%- else -%}
                              {{ block.settings.button_title | split: '|' | first }}
                              {%- endif -%}
                            </a>
                          </div>
                        </div>
                        {%- endif -%}
                        {%- if block.settings.button_title != '' -%}
                        <div class="info-btn-wrapper">
                          <div class="woodmart-button-wrapper text-{{block.settings.content_text_align}}">
                            <a class="btn btn-color-{{block.settings.button_color}} btn-scheme-light btn-scheme-hover-light btn-style-{{block.settings.button_style}} btn-shape-rectangle btn-size-{{block.settings.button_size }}">
                              {%- if settings.language_enable -%}
                              <span class="lang1">{{ block.settings.button_title | split: '|' | first }}</span>
                              <span class="lang2">{{ block.settings.button_title | split: '|' | last }}</span>
                              {%- else -%}
                              {{ block.settings.button_title | split: '|' | first }}
                              {%- endif -%}
                            </a>
                          </div>
                        </div>
                        {%- endif -%}
                      </div>
                    </div>
                  </div>
                </div>
                <style>
                  .vc_custom_{{section.id}} .vc_box_custom_{{ block.id }} {
                    background-image: url({{block.settings.box_background_image | img_url: 'master'}}) !important;
                    background-position: {{block.settings.box_background_position | replace: '_', ' ' }} !important;
                    background-repeat: no-repeat;
                    background-size: {{block.settings.box_background_size}};
                    {{block.settings.box_custom_css}} 
                  }
                  .vc_custom_{{section.id}} .vc_content_custom_{{ block.id }} {
                    {{block.settings.content_custom_css}}
                  } 
                </style>
              </div>
              {%- else -%}
              <div class="{% if block.settings.item_hidden_tablet != blank %}tablet-bg-img-hidden{% endif %}{% if block.settings.item_hidden_mobile != blank %} mobile-bg-img-hidden{% endif %} wpb_column vc_column_container vc_col-sm-{{block.settings.item_width_mobile}} vc_col-lg-{{block.settings.item_width_desktop}} vc_col-md-{{block.settings.item_width_tablet}} vc_col-has-fill{%if block.settings.box_background_position != 'default' %} woodmart-bg-{{block.settings.box_background_position | replace: '_', ' ' }}{% endif %}">
                <div class="vc_column-inner vc_box_custom_{{ block.id }}">
                  <div class="wpb_wrapper"> 
                    <div id="woodmart-title-id-{{ block.id }}" class="title-wrapper  woodmart-title-color-{{block.settings.content_color}} woodmart-title-style-default woodmart-title-size-{{block.settings.title_size}} woodmart-title-width-{{block.settings.content_width}} text-{{block.settings.content_text_align}} vc_content_custom_{{ block.id }}">
                      {%- if block.settings.sub_title != '' -%}
                      <div class="title-subtitle font-alt style-{{block.settings.subtitle_style}}">
                        {%- if settings.language_enable -%}
                        <span class="lang1">{{ block.settings.sub_title | split: '|' | first }}</span>
                        <span class="lang2">{{ block.settings.sub_title | split: '|' | last }}</span>
                        {%- else -%}
                        {{ block.settings.sub_title | split: '|' | first }}
                        {%- endif -%}
                      </div>
                      {%- endif -%}
                      {%- if block.settings.title != '' -%}
                      <div class="liner-continer">
                        <span class="left-line"></span>
                        <h4 class="woodmart-title-container title  woodmart-font-weight-{{block.settings.title_font_weight}}">
                          {%- if settings.language_enable -%}
                          <span class="lang1">{{ block.settings.title | split: '|' | first }}</span>
                          <span class="lang2">{{ block.settings.title | split: '|' | last }}</span>
                          {%- else -%}
                          {{ block.settings.title | split: '|' | first }}
                          {%- endif -%}
                        </h4>
                        <span class="right-line"></span>
                      </div>
                      {%- if block.settings.title_size == 'custom' -%}
                      <style>#woodmart-title-id-{{ block.id }} .woodmart-title-container{font-size:{{block.settings.title_size_custom_desktop | remove: 'px' | append: 'px'}};line-height:{{block.settings.title_size_custom_desktop | remove: 'px' | plus: 10 | append: 'px'}};}{% if block.settings.title_size_custom_tablet != '' %}@media (max-width: 1024px){#woodmart-title-id-{{ block.id }} .woodmart-title-container{font-size:{{block.settings.title_size_custom_tablet | remove: 'px' | append: 'px'}};line-height:{{block.settings.title_size_custom_tablet | remove: 'px' | plus: 10 | append: 'px'}};}{% endif %}{% if block.settings.title_size_custom_mobile != '' %}@media (max-width: 767px){#woodmart-title-id-{{ block.id }} .woodmart-title-container{font-size:{{block.settings.title_size_custom_mobile | remove: 'px' | append: 'px'}};line-height:{{block.settings.title_size_custom_mobile | remove: 'px' | plus: 10 | append: 'px'}};}{% endif %}</style>
                      {%- endif -%}
                      {%- endif -%}
                    </div>
                    {%- if block.settings.content != '' -%}
                    <style>#woodmart-text-block-id-{{block.id}} .woodmart-text-block{font-size:16px;line-height:26px}@media (max-width: 1024px){#woodmart-text-block-id-{{block.id}} .woodmart-text-block{font-size:14px;line-height:24px}}@media (max-width: 767px){#woodmart-text-block-id-{{block.id}} .woodmart-text-block{font-size:14px;line-height:24px}}</style>
                    <div id="woodmart-text-block-id-{{ block.id }}" class="woodmart-text-block-wrapper color-scheme- woodmart-title-size-{{block.settings.content_size}} woodmart-title-width-{{block.settings.content_width}} text-{{block.settings.content_text_align}} vc_custom_content_{{block.id}}">
                      <div class="woodmart-title-container woodmart-text-block font-text woodmart-font-weight-400">
                        {%- if settings.language_enable -%}
                        <div class="lang1">{{ block.settings.content | split: '[lang2]' | first }}</div>
                        <div class="lang2">{{ block.settings.content | split: '[lang2]' | last }}</div>
                        {%- else -%}
                        {{ block.settings.content | split: '[lang2]' | first }}
                        {%- endif -%}
                      </div>
                    </div>
                    {%- endif -%}
                    {%- if block.settings.button_title != '' -%}
                    <div class="woodmart-button-wrapper text-{{block.settings.content_text_align}}">
                      <a class="btn btn-color-{{block.settings.button_color}} btn-scheme-light btn-scheme-hover-light btn-style-{{block.settings.button_style}} btn-shape-rectangle btn-size-{{block.settings.button_size}}">
                        {%- if settings.language_enable -%}
                        <span class="lang1">{{ block.settings.button_title | split: '|' | first }}</span>
                        <span class="lang2">{{ block.settings.button_title | split: '|' | last }}</span>
                        {%- else -%}
                        {{ block.settings.button_title | split: '|' | first }}
                        {%- endif -%}
                      </a>
                    </div> 
                    {%- endif -%}
                    
                  </div>
                </div>
                <style>
                  .vc_custom_{{section.id}} .vc_box_custom_{{ block.id }} {
                    background-image: url({{block.settings.box_background_image | img_url: 'master'}}) !important;
                    background-position: {{block.settings.box_background_position | replace: '_', ' ' }} !important;
                    background-repeat: no-repeat;
                    background-size: {{block.settings.box_background_size}};
                    {{block.settings.box_custom_css}} 
                  } 
                </style>
              </div>
              {%- endcase -%}
              {%- endfor -%}
            </div> 
          {%- endif -%}
        </div>
      </div>
    </div>
  </div>
  {%- if section.settings.section_layout != '0' -%}
  <div class="vc_row-full-width vc_clearfix"></div>
  {%- endif -%}
  <style>
    .vc_custom_{{section.id}} { 
      border-top-width: 0px !important; 
      {%- if section.settings.background_image != blank -%}
      background-image: url({{section.settings.background_image | img_url: 'master'}}) !important;
      background-position: {{section.settings.background_position | replace: '_', ' ' }} !important;
      background-repeat: {{section.settings.background_image_repeat}} !important;
      background-size: {{section.settings.background_size}} !important;
      background-attachment: {{section.settings.background_attachment}} !important;
      {%- endif -%}
      background-color: {{section.settings.background_color}};
    }   
  </style>
</div>
{% schema %}
{
  "name": "Info Box Content",
  "max_blocks": 4,
  "settings": [    
    {
      "type": "header", 
      "content": "Section Settings"
    },  
    {
      "type": "select",
      "id": "section_layout",
      "label": "Layout",
      "options": [
        {
          "value": "0",
          "label": "Default"
        },
        {
          "value": "1",
          "label": "Fullwidth row"
        },
        {
          "value": "2",
          "label": "Fullwidth row and content"
        },
        {
          "value": "3",
          "label": "Fullwidth row and content (no padding)"
        }
      ],
      "default": "0"
    },
    {
      "type": "html",
      "id": "background_gradient_color",
      "label": "Background Html Gradient color"
    },
    {
      "type": "color",
      "id": "background_color",
      "label": "Background color", 
	  "default": "transparent"
    },
    {
      "type": "image_picker",
      "id": "background_image",
      "label": "Section background image" 
    },
    {
      "type": "checkbox",
      "id": "hide_background_mobile",
      "label": "Hide background image on mobile?", 
	  "default": false
    },
    {
      "type": "select",
      "id": "background_image_repeat",
      "label": "Background Repeat",
      "options": [
        {
          "value": "no-repeat",
          "label": "No Repeat"
        },
        {
          "value": "repeat-x",
          "label": "Repeat x (Repeat Horizontal)"
        },
        {
          "value": "repeat-y",
          "label": "Repeat y (Repeat Verticle)"
        },
        {
          "value": "repeat",
          "label": "Repeat All"
        }
      ],
      "default": "no-repeat"
    },
    {
      "type": "select",
      "id": "background_size",
      "label": "Background Size",
      "options": [
        {
          "value": "default",
          "label": "Default"
        },
        {
          "value": "cover",
          "label": "Cover"
        },
        {
          "value": "contain",
          "label": "Contain"
        }
      ],
      "default": "cover"
    },
    {
      "type": "select",
      "id": "background_attachment",
      "label": "Background Attachment",
      "options": [
        {
          "value": "default",
          "label": "Default"
        },
        {
          "value": "fixed",
          "label": "Fixed"
        },
        {
          "value": "scroll",
          "label": "Scroll"
        },
        {
          "value": "inherit",
          "label": "Inherit"
        }
      ],
      "default": "default"
    },
    {
      "type": "select",
      "id": "background_position",
      "label": "Background Position",
      "options": [
        {
          "value": "default",
          "label": "Default"
        },
        {
          "value": "left_top",
          "label": "Left Top"
        },
        {
          "value": "left_center",
          "label": "Left Center"
        },
        {
          "value": "left_bottom",
          "label": "Left Bottom"
        },
        {
          "value": "center_top",
          "label": "Center Top"
        },
        {
          "value": "center_center",
          "label": "Center Center"
        },
        {
          "value": "center_bottom",
          "label": "Center Bottom"
        }
      ],
      "default": "center_center"
    },
    {
      "type": "text",
      "id": "margin_top",
      "label": "Section Margin Top", 
	  "info": "40px"
    },
    {
      "type": "text",
      "id": "margin_bottom",
      "label": "Section Margin Bottom", 
	  "info": "40px"
    },
    {
      "type": "text",
      "id": "padding",
      "label": "Section Padding",
	  "info": "Ex:(40px 40px 40px) (top right bottom left)"
    },
    {
      "type": "text",
      "id": "heading_top",
      "label": "Heading Top" 
    },
    {
      "type": "text",
      "id": "title",
      "label": "Heading" 
    },
    {
      "type": "textarea",
      "id": "heading_content",
      "label": "Content"
    },  
    {
      "type": "select",
      "id": "heading_style",
      "label": "Heading Style",
      "default": "default",
      "options": [
        {
          "value": "default",
          "label": "Default"
        },
        {
          "value": "simple",
          "label": "Simple"
        },
        {
          "value": "bordered",
          "label": "Bordered"
        },
        {
          "value": "underlined",
          "label": "Underlined"
        },
        {
          "value": "shadow",
          "label": "Shadow"
        } 
      ]
    },
    {
      "type": "select",
      "id": "heading_color",
      "label": "Heading Color",
      "default": "default",
      "options": [
        {
