{%- case section.settings.section_layout -%}
{%- when '0' -%}
{%- capture data_fullwidth -%}{%- endcapture -%}
{%- when '1' -%}
{%- capture data_fullwidth -%}data-vc-full-width="true" data-vc-full-width-init="true"{%- endcapture -%}
{%- when '2' -%}
{%- capture data_fullwidth -%}data-vc-full-width="true" data-vc-full-width-init="true" data-vc-stretch-content="true"{%- endcapture -%}
{%- when '3' -%}
{%- capture data_fullwidth -%}data-vc-full-width="true" data-vc-full-width-init="true" data-vc-stretch-content="true"{%- endcapture -%}
{%- endcase -%} 
<div data-section-id="{{ section.id }}" data-section-type="video-poster-section">
  <div {{data_fullwidth}} class="vc_row wpb_row vc_row-fluid {% if section.settings.hide_background_mobile != blank %}mobile-bg-img-hidden{% endif %} vc_custom_{{section.id}} vc_row-has-fill woodmart-bg-center-center {% if section.settings.section_layout == '3' %}vc_row-no-padding{% endif %} woodmart-bg-left-top" style="margin-top:{{section.settings.margin_top | remove: 'px' | append: 'px' }}; margin-bottom:{{section.settings.margin_bottom | remove: 'px' | append: 'px' }};padding: {{section.settings.padding}};">
    <div class="wpb_column vc_column_container vc_col-sm-12">
      <div class="vc_column-inner">
        <div id="video_poster_{{section.id}}" class="wpb_wrapper"> 
          <div class="vc_row wpb_row vc_row-fluid vc_row-o-content-{{section.settings.content_pos}} vc_row-flex">
            {%- if section.blocks.size > 0 -%}
            {%- for block in section.blocks -%}
            {%- case block.type -%} 
            {%- when 'member' -%}
            <div class="{{block.settings.extra_class}} wpb_column vc_column_container vc_col-lg-{{block.settings.column_width_desktop}} vc_col-md-{{block.settings.column_width_tablet}} vc_col-sm-{{block.settings.column_width_mobile}} vc_col-xs-{{block.settings.column_width_mobile}}{% if block.settings.column_hidden_tablet != blank %} vc_hidden-md{% endif %}{% if block.settings.column_hidden_mobile != blank %} vc_hidden-xs{% endif%}{%if block.settings.background_position != 'default' %} woodmart-bg-{{block.settings.background_position | replace: '_', ' ' }}{% endif %} text-{{block.settings.column_text_alignment}}">
              <div class="vc_column-inner vc_custom_{{block.id}}1">
                <div class="wpb_wrapper">
                  <div class="team-member text-{{block.settings.content_alignment}}  member-layout-{{block.settings.social_layout}} color-scheme-{{block.settings.content_color}}">
                    <div class="member-image-wrapper">
                      <div class="member-image">
                        {%- if block.settings.avata_size contains 'x' -%}
                          {%- assign image_width = block.settings.avata_size | split:'x' | first -%}  
                          {%- assign image_height = block.settings.avata_size | split:'x' | last -%}  
                        {%- else -%}
                          {%- assign image_width = block.settings.avata_size -%}  
                          {%- assign image_height = '' -%}  
                        {%- endif -%}
                        {%- capture image_size -%}{{ image_width }}x{{ image_height }}{%- endcapture -%}
                        {%- if block.settings.avata != blank -%}
                        {%- assign img_url = block.settings.avata | img_url: image_size, crop: 'center' -%}
                        {%- else -%}
                        {%- assign img_url = '276x410.png' | asset_url -%}
                        {%- endif -%}
                        <img class="team-member-avatar-image " src="{{img_url}}"  alt="{{block.settings.avata.alt}}">
                      </div>
                    </div>
                    <div class="member-details">
                      {%- if block.settings.name != '' -%}
                      <h4 class="member-name">{{block.settings.name}}</h4>
                      {%- endif -%}
                      {%- if block.settings.title != '' -%}
                      <span class="member-position">{{block.settings.title}}</span>
                      {%- endif -%}
                      {%- if block.settings.content != '' -%}
                      <div class="member-bio">{{block.settings.content}}</div>
                      {%- endif -%}
                      {%- if block.settings.link_facebook != '' or block.settings.link_twitter != '' or block.settings.link_google != '' or block.settings.link_linkedin != '' or block.settings.link_instagram != '' -%}
                      <div class="member-social">
                        <div class="woodmart-social-icons icons-design-{{block.settings.social_button_style}} icons-size-{{block.settings.social_button_size}} social-form-{{block.settings.social_button_form}}">
                          {%- if block.settings.link_facebook != '' -%}
                          <div class="woodmart-social-icon social-facebook">
                            <a class="woodmart-social-icon social-facebook" href="{{block.settings.link_facebook}}">
                              <i class="fa fa-facebook"></i>
                            </a>
                          </div>
                          {%- endif -%}
                          {%- if block.settings.link_twitter != '' -%}
                          <div class="woodmart-social-icon social-twitter">
                            <a class="woodmart-social-icon social-twitter" href="{{block.settings.link_twitter}}">
                              <i class="fa fa-twitter"></i>
                            </a>
                          </div>
                          {%- endif -%}
                          {%- if block.settings.link_google != '' -%}
                          <div class="woodmart-social-icon social-google-plus">
                            <a class="woodmart-social-icon social-google-plus" href="{{block.settings.link_google}}">
                              <i class="fa fa-google-plus"></i>
                            </a>
                          </div>
                          {%- endif -%}
                          {%- if block.settings.link_linkedin != '' -%}
                          <div class="woodmart-social-icon social-linkedin">
                            <a class="woodmart-social-icon social-linkedin" href="{{block.settings.link_linkedin}}">
                              <i class="fa fa-linkedin"></i>
                            </a>
                          </div>
                          {%- endif -%}
                          {%- if block.settings.link_instagram != '' -%}
                          <div class="woodmart-social-icon social-instagram">
                            <a class="woodmart-social-icon social-instagram" href="{{block.settings.link_instagram}}">
                              <i class="fa fa-instagram"></i>
                            </a>
                          </div>
                          {%- endif -%} 
                        </div>
                      </div>
                      {%- endif -%}
                    </div>
                  </div>
                </div>
              </div>
            </div> 
            {%- else -%}
            <div class="{{block.settings.extra_class}} wpb_column vc_column_container vc_col-lg-{{block.settings.column_width_desktop}} vc_col-md-{{block.settings.column_width_tablet}} vc_col-sm-{{block.settings.column_width_mobile}} vc_col-xs-{{block.settings.column_width_mobile}}{% if block.settings.column_hidden_tablet != blank %} vc_hidden-md{% endif %}{% if block.settings.column_hidden_mobile != blank %} vc_hidden-xs{% endif%}{%if block.settings.background_position != 'default' %} woodmart-bg-{{block.settings.background_position | replace: '_', ' ' }}{% endif %} text-{{block.settings.column_text_alignment}}">
              <div class="vc_column-inner vc_custom_{{block.id}}1">
                <div class="wpb_wrapper">
                  {%- if block.settings.sub_title != '' -%}
                  <style>
                    {%- if block.settings.sub_title_font_size == 'custom' -%}
                    #woodmart-text-block-id1-{{section.id}} .woodmart-text-block{font-size:{{block.settings.sub_title_font_size_custom | remove: 'px' | append: 'px'}};line-height:{{block.settings.sub_title_font_size_custom | remove: 'px' | plus: 10 | append: 'px'}};}
                    {%- if block.settings.sub_title_font_size_custom_tablet != '' -%}
                    @media (max-width: 1024px){#woodmart-text-block-id1-{{section.id}} .woodmart-text-block{font-size:{{block.settings.sub_title_font_size_custom_tablet | remove: 'px' | append: 'px'}};line-height:{{block.settings.sub_title_font_size_custom_tablet | remove: 'px' | plus: 10 | append: 'px'}};}}
                    {%- endif -%}
                    {%- if block.settings.sub_title_font_size_custom_mobile != '' -%}
                    @media (max-width: 767px){#woodmart-text-block-id1-{{section.id}} .woodmart-text-block{font-size:{{block.settings.sub_title_font_size_custom_mobile | remove: 'px' | append: 'px'}};line-height:{{block.settings.sub_title_font_size_custom_mobile | remove: 'px' | plus: 10 | append: 'px'}};}}
                    {%- endif -%}
                    {%- endif -%}
                    {%- if block.settings.sub_title_color == 'custom' -%}
                    #woodmart-text-block-id1-{{section.id}} .woodmart-text-block{color:{{block.settings.sub_title_custom_color}};}
                    {%- endif -%}
                    .vc_custom_{{section.id}}1 {
                      {%- if block.settings.sub_title_margin_top != '' -%}
                      margin-top: {{block.settings.sub_title_margin_top | remove: 'px' | append: 'px'}};
                      {%- endif -%}
                      {%- if block.settings.sub_title_margin_right != '' -%}
                      margin-right: {{block.settings.sub_title_margin_right | remove: 'px' | append: 'px'}};
                      {%- endif -%}
                      {%- if block.settings.sub_title_margin_bottom != '' -%}
                      margin-bottom: {{block.settings.sub_title_margin_bottom | remove: 'px' | append: 'px'}};
                      {%- endif -%}
                      {%- if block.settings.sub_title_margin_left != '' -%}
                      margin-left: {{block.settings.sub_title_margin_left | remove: 'px' | append: 'px'}};
                      {%- endif -%}
                    }
                  </style>
                  <div id="woodmart-text-block-id1-{{section.id}}" class="woodmart-text-block-wrapper color-scheme-{{block.settings.sub_title_color}} woodmart-title-size-{{block.settings.sub_title_font_size}} woodmart-title-width-{{block.settings.sub_title_width}} text-{{block.settings.sub_title_align}} vc_custom_{{section.id}}1">
                    <div class="woodmart-title-container woodmart-text-block font-{{block.settings.sub_title_font}} woodmart-font-weight-{{block.settings.sub_title_font_weight}}">
                      {%- if settings.language_enable -%}
                      <span class="lang1">{{ block.settings.sub_title | split: '|' | first }}</span>
                      <span class="lang2">{{ block.settings.sub_title | split: '|' | last }}</span>
                      {%- else -%}
                      {{ block.settings.sub_title | split: '|' | first }}
                      {%- endif -%}
                    </div>
                  </div>
                  {%- endif -%}
                  {%- if block.settings.title != '' -%}
                  <style>
                    {%- if block.settings.title_font_size == 'custom' -%}
                    #woodmart-text-block-id2-{{section.id}} .woodmart-text-block{font-size:{{block.settings.title_font_size_custom | remove: 'px' | append: 'px'}};line-height:{{block.settings.title_font_size_custom | remove: 'px' | plus: 10 | append: 'px'}};}
                    {%- if block.settings.title_font_size_custom_tablet != '' -%}
                    @media (max-width: 1024px){#woodmart-text-block-id2-{{section.id}} .woodmart-text-block{font-size:{{block.settings.title_font_size_custom_tablet | remove: 'px' | append: 'px'}};line-height:{{block.settings.title_font_size_custom_tablet | remove: 'px' | plus: 10 | append: 'px'}};}}
                    {%- endif -%}
                    {%- if block.settings.title_font_size_custom_mobile != '' -%}
                    @media (max-width: 767px){#woodmart-text-block-id2-{{section.id}} .woodmart-text-block{font-size:{{block.settings.title_font_size_custom_mobile | remove: 'px' | append: 'px'}};line-height:{{block.settings.title_font_size_custom_mobile | remove: 'px' | plus: 10 | append: 'px'}};}}
                    {%- endif -%}
                    {%- endif -%}
                    {%- if block.settings.title_color == 'custom' -%}
                    #woodmart-text-block-id2-{{section.id}} .woodmart-text-block{color:{{block.settings.title_custom_color}};}
                    {%- endif -%}
                    .vc_custom_{{section.id}}2 {
                      {%- if block.settings.title_margin_top != '' -%}
                      margin-top: {{block.settings.title_margin_top | remove: 'px' | append: 'px'}};
                      {%- endif -%}
                      {%- if block.settings.title_margin_right != '' -%}
                      margin-right: {{block.settings.title_margin_right | remove: 'px' | append: 'px'}};
                      {%- endif -%}
                      {%- if block.settings.title_margin_bottom != '' -%}
                      margin-bottom: {{block.settings.title_margin_bottom | remove: 'px' | append: 'px'}};
                      {%- endif -%}
                      {%- if block.settings.title_margin_left != '' -%}
                      margin-left: {{block.settings.title_margin_left | remove: 'px' | append: 'px'}};
                      {%- endif -%}
                    }
                  </style>
                  <div id="woodmart-text-block-id2-{{section.id}}" class="woodmart-text-block-wrapper color-scheme-{{block.settings.title_color}} woodmart-title-size-{{block.settings.title_font_size}} woodmart-title-width-{{block.settings.title_width}} text-{{block.settings.title_align}} vc_custom_{{section.id}}2">
                    <div class="woodmart-title-container woodmart-text-block font-{{block.settings.title_font}} woodmart-font-weight-{{block.settings.title_font_weight}}">
                      {%- if settings.language_enable -%}
                      <span class="lang1">{{ block.settings.title | split: '|' | first }}</span>
                      <span class="lang2">{{ block.settings.title | split: '|' | last }}</span>
                      {%- else -%}
                      {{ block.settings.title | split: '|' | first }}
                      {%- endif -%}
                    </div>
                  </div>
                  {%- endif -%}
                  {%- if block.settings.title2 != '' -%}
                  <style>
                    {%- if block.settings.title2_font_size == 'custom' -%}
                    #woodmart-text-block-id3-{{section.id}} .woodmart-text-block{font-size:{{block.settings.title2_font_size_custom | remove: 'px' | append: 'px'}};line-height:{{block.settings.title2_font_size_custom | remove: 'px' | plus: 10 | append: 'px'}};}
                    {%- if block.settings.title2_font_size_custom_tablet != '' -%}
                    @media (max-width: 1024px){#woodmart-text-block-id3-{{section.id}} .woodmart-text-block{font-size:{{block.settings.title2_font_size_custom_tablet | remove: 'px' | append: 'px'}};line-height:{{block.settings.title2_font_size_custom_tablet | remove: 'px' | plus: 10 | append: 'px'}};}}
                    {%- endif -%}
                    {%- if block.settings.title2_font_size_custom_mobile != '' -%}
                    @media (max-width: 767px){#woodmart-text-block-id3-{{section.id}} .woodmart-text-block{font-size:{{block.settings.title2_font_size_custom_mobile | remove: 'px' | append: 'px'}};line-height:{{block.settings.title2_font_size_custom_mobile | remove: 'px' | plus: 10 | append: 'px'}};}}
                    {%- endif -%}
                    {%- endif -%}
                    {%- if block.settings.title2_color == 'custom' -%}
                    #woodmart-text-block-id3-{{section.id}} .woodmart-text-block{color:{{block.settings.title2_custom_color}};}
                    {%- endif -%}
                    .vc_custom_{{section.id}}3 {
                      {%- if block.settings.title2_margin_top != '' -%}
                      margin-top: {{block.settings.title2_margin_top | remove: 'px' | append: 'px'}};
                      {%- endif -%}
                      {%- if block.settings.title2_margin_right != '' -%}
                      margin-right: {{block.settings.title2_margin_right | remove: 'px' | append: 'px'}};
                      {%- endif -%}
                      {%- if block.settings.title2_margin_bottom != '' -%}
                      margin-bottom: {{block.settings.title2_margin_bottom | remove: 'px' | append: 'px'}};
                      {%- endif -%}
                      {%- if block.settings.title2_margin_left != '' -%}
                      margin-left: {{block.settings.title2_margin_left | remove: 'px' | append: 'px'}};
                      {%- endif -%}
                    }
                  </style>
                  <div id="woodmart-text-block-id3-{{section.id}}" class="woodmart-text-block-wrapper color-scheme-{{block.settings.title2_color}} woodmart-title-size-{{block.settings.title2_font_size}} woodmart-title-width-{{block.settings.title2_width}} text-{{block.settings.title2_align}} vc_custom_{{section.id}}3">
                    <div class="woodmart-title-container woodmart-text-block font-{{block.settings.title2_font}} woodmart-font-weight-{{block.settings.title2_font_weight}}">
                      {%- if settings.language_enable -%}
                      <span class="lang1">{{ block.settings.title2 | split: '|' | first }}</span>
                      <span class="lang2">{{ block.settings.title2 | split: '|' | last }}</span>
                      {%- else -%}
                      {{ block.settings.title2 | split: '|' | first }}
                      {%- endif -%}
                    </div>
                  </div>
                  {%- endif -%}
                  {%- if block.settings.content != '' -%}
                  <div class="content woodmart-text-block-wrapper text-{{block.settings.content_align}} woodmart-title-width-{{block.settings.content_width}}" style="color:{{block.settings.content_color}};margin-bottom: {{block.settings.content_margin_bottom | remove: 'px' | append: 'px'}};">
                    <div class="woodmart-title-container">
                      {%- if settings.language_enable -%}
                      <div class="lang1">{{ block.settings.content | split: '[lang2]' | first }}</div>
                      <div class="lang2">{{ block.settings.content | split: '[lang2]' | last }}</div>
                      {%- else -%}
                      {{ block.settings.content | split: '[lang2]' | first }}
                      {%- endif -%}
                    </div>
                  </div> 
                  {%- endif -%}
                  {%- if block.settings.button_title_1 != '' -%}
                  <div class="woodmart-button-wrapper text-center btn-inline">
                    <a href="{{block.settings.button_link_1}}" title="" class="btn btn-color-{{block.settings.button_color_1}} btn-style-{{block.settings.button_style_1}} btn-size-{{block.settings.button_size_1}}">
                      {%- if settings.language_enable -%}
                      <span class="lang1">{{ block.settings.button_title_1 | split: '|' | first }}</span>
                      <span class="lang2">{{ block.settings.button_title_1 | split: '|' | last }}</span>
                      {%- else -%}
                      {{ block.settings.button_title_1 | split: '|' | first }}
                      {%- endif -%}
                    </a>
                  </div>
                  {%- endif -%}
                  {%- if block.settings.button_title_2 != '' -%}
                  <div class="woodmart-button-wrapper text-center btn-inline">
                    <a href="{{block.settings.button_link_2}}" title="" class="btn btn-color-{{block.settings.button_color_2}} btn-style-{{block.settings.button_style_2}} btn-size-{{block.settings.button_size_2}}">
                      {%- if settings.language_enable -%}
                      <span class="lang1">{{ block.settings.button_title_2 | split: '|' | first }}</span>
                      <span class="lang2">{{ block.settings.button_title_2 | split: '|' | last }}</span>
                      {%- else -%}
                      {{ block.settings.button_title_2 | split: '|' | first }}
                      {%- endif -%}
                    </a>
                  </div>
                  {%- endif -%}
                </div> 
              </div>
            </div> 
            {%- endcase -%}
            <style>
              .vc_custom_{{block.id}}1 { 
                {%- if block.settings.background_color != blank -%}
                background-color: {{block.settings.background_color}} !important;
                {%- endif -%}
                {%- if block.settings.background_image != blank -%}
                background-image: url({{block.settings.background_image | img_url: 'master' }}) !important;
                background-position: {{block.settings.background_position | replace: '_', ' ' }} !important;
                background-repeat: no-repeat !important;
                background-size: cover !important;
                {%- endif -%}
                {%- if block.settings.border_integer != blank -%}
                border-width: {{block.settings.border_integer | remove: ' ' | split: '-' | join: ' '}} !important;
                border-color: {{block.settings.border_color}} !important;
                border-style: {{block.settings.border_style}} !important;
                {%- endif -%}
                {%- if block.settings.margin_top != blank -%}
                margin-top: {{block.settings.margin_top | remove: 'px' | append: 'px'}} !important;
                {%- endif -%}
                {%- if block.settings.margin_bottom != blank -%}
                margin-bottom: {{block.settings.margin_bottom | remove: 'px' | append: 'px'}} !important;
                {%- endif -%}
                {%- if block.settings.padding_top != blank -%}
                padding-top: {{block.settings.padding_top | remove: 'px' | append: 'px'}} !important;
                {%- endif -%}
                {%- if block.settings.padding_right != blank -%}
                padding-right: {{block.settings.padding_right | remove: 'px' | append: 'px'}} !important;
                {%- endif -%}
                {%- if block.settings.padding_bottom != blank -%}
                padding-bottom: {{block.settings.padding_bottom | remove: 'px' | append: 'px'}} !important;
                {%- endif -%}
                {%- if block.settings.padding_left != blank -%}
                padding-left: {{block.settings.padding_left | remove: 'px' | append: 'px'}} !important;
                {%- endif -%}
              } 
            </style>
            {%- endfor -%}
            {%- endif -%} 
          </div>
        </div>
      </div>
    </div> 
    <style>
      .vc_custom_{{section.id}} {  
        {%- if section.settings.background_image != blank -%}
        background-image: url({{section.settings.background_image | img_url: 'master'}}) !important;
        background-position: {{section.settings.background_position | replace: '_', ' ' }} !important;
        background-repeat: {{section.settings.background_image_repeat}} !important;
        background-size: {{section.settings.background_size}} !important;
        background-attachment: {{section.settings.background_attachment}} !important;
        {%- endif -%}
        background-color: {{section.settings.background_color}};
      } 
    </style>
  </div>
  {%- if section.settings.section_layout != '0' -%}
  <div class="vc_row-full-width vc_clearfix"></div>
  {%- endif -%} 
</div>
{% schema %}
{
  "name": "Team Member",
  "settings": [ 
    {
      "type": "header", 
      "content": "Section Settings"
    },
    {
      "type": "select",
      "id": "section_layout",
      "label": "Layout",
      "options": [
        {
          "value": "0",
          "label": "Default"
        },
        {
          "value": "1",
          "label": "Fullwidth row"
        },
        {
          "value": "2",
          "label": "Fullwidth row and content"
        },
        {
          "value": "3",
          "label": "Fullwidth row and content (no padding)"
        }
      ],
      "default": "1"
    },
    {
      "type": "color",
      "id": "background_color",
      "label": "Background color", 
	  "default": "transparent"
    },
    {
      "type": "image_picker",
      "id": "background_image",
      "label": "Section background image" 
    },
    {
      "type": "checkbox",
      "id": "hide_background_mobile",
      "label": "Hide background image on mobile?", 
	  "default": true
    },
    {
      "type": "select",
      "id": "background_image_repeat",
      "label": "Background Repeat",
      "options": [
        {
          "value": "no-repeat",
          "label": "No Repeat"
        },
        {
          "value": "repeat-x",
          "label": "Repeat x (Repeat Horizontal)"
        },
        {
          "value": "repeat-y",
          "label": "Repeat y (Repeat Verticle)"
        },
        {
          "value": "repeat",
          "label": "Repeat All"
        }
      ],
      "default": "no-repeat"
    },
    {
      "type": "select",
      "id": "background_size",
      "label": "Background Size",
      "options": [
        {
          "value": "default",
          "label": "Default"
        },
        {
          "value": "cover",
          "label": "Cover"
        },
        {
          "value": "contain",
          "label": "Contain"
        }
      ],
      "default": "cover"
    },
    {
      "type": "select",
      "id": "background_attachment",
      "label": "Background Attachment",
      "options": [
        {
          "value": "default",
          "label": "Default"
        },
        {
          "value": "fixed",
          "label": "Fixed"
        },
        {
          "value": "scroll",
          "label": "Scroll"
        },
        {
          "value": "inherit",
          "label": "Inherit"
        }
      ],
      "default": "default"
    },
    {
      "type": "select",
      "id": "background_position",
      "label": "Background Position",
      "options": [
        {
          "value": "default",
          "label": "Default"
        },
        {
          "value": "left_top",
          "label": "Left Top"
        },
        {
