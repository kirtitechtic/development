{%- case section.settings.section_layout -%}
{%- when '0' -%}
{%- capture data_fullwidth -%}{%- endcapture -%}
{%- when '1' -%}
{%- capture data_fullwidth -%}data-vc-full-width="true" data-vc-full-width-init="true"{%- endcapture -%}
{%- when '2' -%}
{%- capture data_fullwidth -%}data-vc-full-width="true" data-vc-full-width-init="true" data-vc-stretch-content="true"{%- endcapture -%}
{%- when '3' -%}
{%- capture data_fullwidth -%}data-vc-full-width="true" data-vc-full-width-init="true" data-vc-stretch-content="true"{%- endcapture -%}
{%- endcase -%} 
<div data-section-id="{{ section.id }}" data-section-type="instagram-section">
  <div {{data_fullwidth}} class="vc_row-flex text-section spb_column {% if section.settings.hide_background_mobile != blank %}mobile-bg-img-hidden{% endif %} vc_custom_{{section.id}} vc_row {% if section.settings.section_layout == '3' %}vc_row-no-padding{% endif %}"  style="margin-top: {{section.settings.margin_top | remove: 'px' | append: 'px' }};margin-bottom: {{section.settings.margin_bottom | remove: 'px' | append: 'px' }};padding: {{section.settings.padding}};">
    <div class="wpb_column vc_column_container vc_col-sm-12">
      <div class="vc_column-inner">
        <div class="wpb_wrapper">
          {%- if section.blocks.size > 0 -%} 
          <div class="vc_row wpb_row vc_row-fluid vc_custom_{{section.id}}8 vc_row-o-content-middle vc_row-flex">
            {%- for block in section.blocks -%} 
            {%- case block.type -%} 
            {%- when 'logo' -%}
            <div class="vc_col-has-fill wpb_column vc_column_container vc_col-lg-{{block.settings.column_width_desktop}} vc_col-md-{{block.settings.column_width_tablet}} vc_col-sm-{{block.settings.column_width_tablet}} vc_col-xs-{{block.settings.column_width_mobile}}{% if block.settings.column_hidden_tablet != blank %} vc_hidden-md{% endif %}{% if block.settings.column_hidden_mobile != blank %} vc_hidden-xs{% endif%} text-{{block.settings.column_text_alignment}}">
              <div class="vc_column-inner vc_custom_{{block.id}}7">
                <div class="wpb_wrapper">
                  <div class="footer-logo" style="max-width: 80%;margin-bottom: 10px;">
                    {%- case block.settings.logo_width -%}
                    {%- when '' -%}
                      {%- assign logo_width = '220' -%}
                      {%- assign logo_height = '' -%} 
                    {%- else -%}
                      {%- assign logo_width = block.settings.logo_width -%}
                      {%- assign logo_height = '' -%}
                    {%- endcase -%}  
                    {%- capture logo_size -%}{{ logo_width }}x{{ logo_height }}{%- endcapture -%} 
                    {%- if block.settings.logo_svg != '' -%}
                    <img class="logo" src="{{ block.settings.logo_svg | asset_url }}" alt="{{ image.alt | default: shop.name }}" style="width: {{block.settings.logo_width | remove: "px" | append: "px"}}"/>
                    {%- else -%}
                    {%- if block.settings.logo != blank -%} 
                    <img class="logo" src="{{ block.settings.logo | img_url: logo_size }}" alt="{{ image.alt | default: shop.name }}"  style="width: {{block.settings.logo_width | remove: "px" | append: "px"}}"/> 
                    {%- else -%}
                    {{shop.name}}
                    {%- endif -%}
                    {%- endif -%}
                  </div>
                </div>
              </div> 
              <style>
                .vc_custom_{{ block.id }}7 {  
                  {%- if block.settings.border_integer != blank -%}
                  border-width: {{block.settings.border_integer | remove: ' ' | split: '-' | join: ' '}} !important;
                  border-color: {{block.settings.border_color}} !important;
                  border-style: {{block.settings.border_style}} !important;
                  {%- endif -%}
                  {%- if block.settings.margin_top != blank -%}
                  margin-top: {{block.settings.margin_top | remove: 'px' | append: 'px'}} !important;
                  {%- endif -%}
                  {%- if block.settings.margin_bottom != blank -%}
                  margin-bottom: {{block.settings.margin_bottom | remove: 'px' | append: 'px'}} !important;
                  {%- endif -%}
                  {%- if block.settings.padding_top != blank -%}
                  padding-top: {{block.settings.padding_top | remove: 'px' | append: 'px'}} !important;
                  {%- endif -%}
                  {%- if block.settings.padding_right != blank -%}
                  padding-right: {{block.settings.padding_right | remove: 'px' | append: 'px'}} !important;
                  {%- endif -%}
                  {%- if block.settings.padding_bottom != blank -%}
                  padding-bottom: {{block.settings.padding_bottom | remove: 'px' | append: 'px'}} !important;
                  {%- endif -%}
                  {%- if block.settings.padding_left != blank -%}
                  padding-left: {{block.settings.padding_left | remove: 'px' | append: 'px'}} !important;
                  {%- endif -%}
                } 
              </style>
            </div>
            {%- when 'menu' -%}
            <div class="vc_col-has-fill wpb_column vc_column_container vc_col-lg-{{block.settings.column_width_desktop}} vc_col-md-{{block.settings.column_width_tablet}} vc_col-sm-{{block.settings.column_width_tablet}} vc_col-xs-{{block.settings.column_width_mobile}}{% if block.settings.column_hidden_tablet != blank %} vc_hidden-md{% endif %}{% if block.settings.column_hidden_mobile != blank %} vc_hidden-xs{% endif%} text-{{block.settings.column_text_alignment}}">
              <div class="vc_column-inner vc_custom_{{block.id}}7">
                <div class="wpb_wrapper">
                  <div class="wpb_raw_code wpb_content_element wpb_raw_html vc_custom_{{block.id}}1" style="margin:0;">
                    <div class="wpb_wrapper">
                      {%- assign linklist = linklists[block.settings.menu] -%}
                      {%- if linklist.links.size > 0 -%}
                      <ul class="inline-list main-nav-style">
                        {%- for link in linklist.links -%}
                        <li>
                          <a href="{{ link.url }}">
                            {% if settings.language_enable %}
                            <span class="lang1">{{ link.title | split: '|' | first }}</span>
                            <span class="lang2">{{ link.title | split: '|' | last }}</span>
                            {% else %}
                            {{ link.title | split: '|' | first }}
                            {% endif %}
                          </a>
                        </li>
                        {%- endfor -%}
                      </ul>
                      {%- endif -%}
                    </div>
                  </div>
                </div>
              </div>
              <style>
                .vc_custom_{{ block.id }}7 {  
                  {%- if block.settings.border_integer != blank -%}
                  border-width: {{block.settings.border_integer | remove: ' ' | split: '-' | join: ' '}} !important;
                  border-color: {{block.settings.border_color}} !important;
                  border-style: {{block.settings.border_style}} !important;
                  {%- endif -%}
                  {%- if block.settings.margin_top != blank -%}
                  margin-top: {{block.settings.margin_top | remove: 'px' | append: 'px'}} !important;
                  {%- endif -%}
                  {%- if block.settings.margin_bottom != blank -%}
                  margin-bottom: {{block.settings.margin_bottom | remove: 'px' | append: 'px'}} !important;
                  {%- endif -%}
                  {%- if block.settings.padding_top != blank -%}
                  padding-top: {{block.settings.padding_top | remove: 'px' | append: 'px'}} !important;
                  {%- endif -%}
                  {%- if block.settings.padding_right != blank -%}
                  padding-right: {{block.settings.padding_right | remove: 'px' | append: 'px'}} !important;
                  {%- endif -%}
                  {%- if block.settings.padding_bottom != blank -%}
                  padding-bottom: {{block.settings.padding_bottom | remove: 'px' | append: 'px'}} !important;
                  {%- endif -%}
                  {%- if block.settings.padding_left != blank -%}
                  padding-left: {{block.settings.padding_left | remove: 'px' | append: 'px'}} !important;
                  {%- endif -%}
                } 
              </style>
            </div>
            {%- else -%}
            <div class="vc_col-has-fill wpb_column vc_column_container vc_col-lg-{{block.settings.column_width_desktop}} vc_col-md-{{block.settings.column_width_tablet}} vc_col-sm-{{block.settings.column_width_tablet}} vc_col-xs-{{block.settings.column_width_mobile}}{% if block.settings.column_hidden_tablet != blank %} vc_hidden-md{% endif %}{% if block.settings.column_hidden_mobile != blank %} vc_hidden-xs{% endif%} text-{{block.settings.column_text_alignment}}">
              <div class="vc_column-inner vc_custom_{{block.id}}7">
                <div class="wpb_wrapper">
                  <div class="woodmart-social-icons text-{{block.settings.social_button_align}} icons-design-{{block.settings.social_button_style}} icons-size-{{block.settings.social_button_size}} color-scheme-{{block.settings.social_button_color}} social-share social-form-{{block.settings.social_button_form}}">
                    {%- assign _url = block.settings.link_share -%}
                    {%- include 'social-sharing', share_url: _url -%}
                  </div>
                </div>
              </div>
              <style>
                .vc_custom_{{ block.id }}7 {  
                  {%- if block.settings.border_integer != blank -%}
                  border-width: {{block.settings.border_integer | remove: ' ' | split: '-' | join: ' '}} !important;
                  border-color: {{block.settings.border_color}} !important;
                  border-style: {{block.settings.border_style}} !important;
                  {%- endif -%}
                  {%- if block.settings.margin_top != blank -%}
