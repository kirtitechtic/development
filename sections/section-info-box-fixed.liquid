                          <div class="wpb_wrapper"> 
                            <div class="info-box-wrapper">
                              <div id="infobox-{{block.id}}2{{i}}" class=" cursor-pointer woodmart-info-box text-left box-icon-align-left box-style- color-scheme- woodmart-bg-none box-title-small vc_custom_1523885224118" style="margin-bottom: 60px;">
                                <div class="box-icon-wrapper  box-with-icon box-icon-simple">
                                  <div class="info-box-icon"> 
                                    {%- if block.settings[icon_type] == 'text' -%}
                                      {{block.settings[icon_text]}}
                                    {%- endif -%}
                                    {%- if block.settings[icon_type] == 'icon' -%}
                                      {%- if block.settings[icon_svg] != '' -%}
                                        {{block.settings[icon_svg]}}
                                      {%- else -%} 
                                        {%- if block.settings[icon_image] != blank -%}
                                        <img src="{{block.settings[icon_image] | img_url: 'master'}}" /> 
                                        {%- endif -%}
                                      {%- endif -%}
                                    {%- endif -%}
                                  </div> 
                                </div>
                                <div class="info-box-content"> 
                                  {%- if block.settings[title] != '' -%}
                                  <h4 class="info-box-title woodmart-font-weight- box-title-style-default">
                                    {%- if settings.language_enable -%}
                                    <span class="lang1">{{ block.settings[title] | split: '|' | first }}</span>
                                    <span class="lang2">{{ block.settings[title] | split: '|' | last }}</span>
                                    {%- else -%}
                                    {{ block.settings[title] | split: '|' | first }}
                                    {%- endif -%}
                                  </h4>
                                  {%- endif -%}
                                  {%- if block.settings[content] != '' -%}
                                  <div class="info-box-inner reset-mb-10">
                                    {%- if settings.language_enable -%}
                                    <div class="lang1">{{ block.settings[content] | split: '[lang2]' | first }}</div>
                                    <div class="lang2">{{ block.settings[content] | split: '[lang2]' | last }}</div>
                                    {%- else -%}
                                    {{ block.settings[content] | split: '[lang2]' | first }}
                                    {%- endif -%}
                                  </div>
                                  {%- endif -%} 
                                </div>
                              </div>
                            </div>
                          </div>
                        </div>
                      </div>
                      {%- endif -%}
                      {%- endfor -%}
                    </div> 
                  </div>
                </div>
                <style>
                  .vc_box_custom_{{ block.id }}7 { 
                    {%- if block.settings.background_color != blank -%}
                    background-color: {{block.settings.background_color}} !important;
                    {%- endif -%}
                    {%- if block.settings.background_image != blank -%}
                    background-image: url({{block.settings.background_image | img_url: 'master' }}) !important;
                    background-position: {{block.settings.background_position | replace: '_', ' ' }} !important;
                    background-repeat: no-repeat !important;
                    background-size: cover !important;
                    {%- endif -%}
                    {%- if block.settings.border_integer != blank -%}
                    border-width: {{block.settings.border_integer | remove: ' ' | split: '-' | join: ' '}} !important;
                    border-color: {{block.settings.border_color}} !important;
                    border-style: {{block.settings.border_style}} !important;
                    {%- endif -%}
                    {%- if block.settings.margin_top != blank -%}
                    margin-top: {{block.settings.margin_top | remove: 'px' | append: 'px'}} !important;
                    {%- endif -%}
                    {%- if block.settings.margin_bottom != blank -%}
                    margin-bottom: {{block.settings.margin_bottom | remove: 'px' | append: 'px'}} !important;
                    {%- endif -%}
                    {%- if block.settings.padding_top != blank -%}
                    padding-top: {{block.settings.padding_top | remove: 'px' | append: 'px'}} !important;
                    {%- endif -%}
                    {%- if block.settings.padding_right != blank -%}
                    padding-right: {{block.settings.padding_right | remove: 'px' | append: 'px'}} !important;
                    {%- endif -%}
                    {%- if block.settings.padding_bottom != blank -%}
                    padding-bottom: {{block.settings.padding_bottom | remove: 'px' | append: 'px'}} !important;
                    {%- endif -%}
                    {%- if block.settings.padding_left != blank -%}
                    padding-left: {{block.settings.padding_left | remove: 'px' | append: 'px'}} !important;
                    {%- endif -%}
                  } 
                </style>
              </div>
              {%- else -%}
              <div class="vc_col-has-fill vc_video-bg-container wpb_column vc_column_container vc_col-lg-{{block.settings.column_width_desktop}} vc_col-md-{{block.settings.column_width_tablet}} vc_col-sm-{{block.settings.column_width_tablet}} vc_col-xs-{{block.settings.column_width_mobile}}{% if block.settings.column_hidden_tablet != blank %} vc_hidden-md{% endif %}{% if block.settings.column_hidden_mobile != blank %} vc_hidden-xs{% endif%} woodmart-parallax{%if block.settings.background_position != 'default' %} woodmart-bg-{{block.settings.background_position | replace: '_', ' ' }}{% endif %} text-{{block.settings.column_text_alignment}}">
                <div class="vc_column-inner vc_box_custom_{{ block.id }}7">
                  <div class="wpb_wrapper"> 
                    <div class="info-box-wrapper {{block.settings.extra_class}}">
                      <div id="infobox-{{block.id}}" class="woodmart-info-box with-btn btn-position-{{block.settings.button_position}}  text-{{block.settings.text_alignment}} box-title-{{block.settings.title_size}} icon-alignment-{{block.settings.image_alignment}} box-{{block.settings.box_style}} color-scheme-{{block.settings.color_scheme}}{% if block.settings.box_style == 'bg-hover'%} hover-color-scheme-{{block.settings.color_scheme_hover}}{% endif %} woodmart-bg-none vc_custom_{{block.id}}5">
                        <div class="box-icon-wrapper with-{{block.settings.icon_type}}{% if block.settings.icon_type == 'text' %} text-size-{{block.settings.icon_text_size}}{% endif %} icon-style-{{block.settings.icon_style}}">
                          <div class="info-box-icon"> 
                            {%- if block.settings.icon_type == 'text' -%}
                            {{block.settings.icon_text}}
                            {%- endif -%}
                            {%- if block.settings.icon_type == 'icon' -%}
                            {%- if block.settings.icon_svg_code != '' -%}
                            {{block.settings.icon_svg_code}}
                            {%- else -%} 
                            {%- if block.settings.icon_image != blank -%}
                            <img src="{{block.settings.icon_image | img_url: 'master'}}" /> 
                            {%- endif -%}
                            {%- endif -%}
                            {%- endif -%}
                          </div> 
                        </div>
                        <div class="info-box-content">
                          {%- if block.settings.sub_title != '' -%}
                          <div class="info-box-subtitle subtitle-style-{{block.settings.subtitle_style}} subtitle-color-{{block.settings.subtitle_color}}">
                            {%- if settings.language_enable -%}
                            <span class="lang1">{{ block.settings.sub_title | split: '|' | first }}</span>
                            <span class="lang2">{{ block.settings.sub_title | split: '|' | last }}</span>
                            {%- else -%}
                            {{ block.settings.sub_title | split: '|' | first }}
                            {%- endif -%}
                          </div>
                          {%- endif -%}
                          {%- if block.settings.title != '' -%}
                          <h4 class="info-box-title">
                            {%- if settings.language_enable -%}
                            <span class="lang1">{{ block.settings.title | split: '|' | first }}</span>
                            <span class="lang2">{{ block.settings.title | split: '|' | last }}</span>
                            {%- else -%}
                            {{ block.settings.title | split: '|' | first }}
                            {%- endif -%}
                          </h4>
                          {%- endif -%}
                          {%- if block.settings.content != '' -%}
                          <div class="info-box-inner">
                            {%- if settings.language_enable -%}
                            <div class="lang1">{{ block.settings.content | split: '[lang2]' | first }}</div>
                            <div class="lang2">{{ block.settings.content | split: '[lang2]' | last }}</div>
                            {%- else -%}
                            {{ block.settings.content | split: '[lang2]' | first }}
                            {%- endif -%}
                          </div>
                          {%- endif -%}
                          {%- if block.settings.button_title != '' -%}
                          <div class="info-btn-wrapper">
                            <div class="woodmart-button-wrapper text-{{block.settings.column_text_alignment}}">
                              <a href="{{block.settings.button_link}}" class="btn btn-color-{{block.settings.button_color}} btn-scheme-light btn-scheme-hover-light btn-style-{{block.settings.button_style}} btn-shape-rectangle btn-size-{{block.settings.button_size }}">
                                {%- if settings.language_enable -%}
                                <span class="lang1">{{ block.settings.button_title | split: '|' | first }}</span>
                                <span class="lang2">{{ block.settings.button_title | split: '|' | last }}</span>
                                {%- else -%}
                                {{ block.settings.button_title | split: '|' | first }}
                                {%- endif -%}
                              </a>
                            </div>
                          </div>
                          {%- endif -%}
                        </div>
                      </div>
                      <style>
                        {%- if block.settings.box_style == 'bg-hover' -%}
                        #infobox-{{block.id}}:hover {
                          background-color: {{block.settings.background_color_hover}} !important;
                        }
                        {%- endif -%}
                        {%- if block.settings.icon_type == 'text' and block.settings.icon_text_color != blank -%}
                        #infobox-{{block.id}} .with-text {
                          color: {{block.settings.icon_text_color}} !important;
                        }
                        {%- endif -%}
                        {%- if block.settings.icon_style == 'with-border' and block.settings.icon_border_color != blank -%}
                        #infobox-{{block.id}} .info-box-icon {
                          border-color: {{block.settings.icon_border_color}} !important;
                        }
                        {%- endif -%}
                        {%- if block.settings.icon_style == 'with-border' and block.settings.icon_border_hover_color != blank -%}
                        #infobox-{{block.id}}:hover .info-box-icon {
                          border-color: {{block.settings.icon_border_hover_color}} !important;
                        }
                        {%- endif -%}
                        {%- if block.settings.icon_style == 'with-bg' and block.settings.icon_background_color != blank -%}
                        #infobox-{{block.id}} .info-box-icon {
                          background-color: {{block.settings.icon_background_color}} !important;
                        }
                        {%- endif -%}
                        {%- if block.settings.icon_style == 'with-bg' and block.settings.icon_background_hover_color != blank -%}
                        #infobox-{{block.id}}:hover .info-box-icon {
                          background-color: {{block.settings.icon_background_hover_color}} !important;
                        }
                        {%- endif -%}
                        .vc_custom_{{block.id}}5 {
