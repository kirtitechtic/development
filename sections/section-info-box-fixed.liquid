{% schema %}
{
  "name": "Infomation Box Fixed",
  "max_blocks": 4,
  "settings": [    
    {
      "type": "header", 
      "content": "Section Settings"
    },  
    {
      "type": "select",
      "id": "section_layout",
      "label": "Layout",
