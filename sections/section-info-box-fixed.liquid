{%- case section.settings.section_layout -%}
{%- when '0' -%}
{%- capture data_fullwidth -%}{%- endcapture -%}
{%- when '1' -%}
{%- capture data_fullwidth -%}data-vc-full-width="true" data-vc-full-width-init="true"{%- endcapture -%}
{%- when '2' -%}
{%- capture data_fullwidth -%}data-vc-full-width="true" data-vc-full-width-init="true" data-vc-stretch-content="true"{%- endcapture -%}
{%- when '3' -%}
{%- capture data_fullwidth -%}data-vc-full-width="true" data-vc-full-width-init="true" data-vc-stretch-content="true"{%- endcapture -%}
{%- endcase -%} 
<div data-section-id="{{ section.id }}" data-section-type="infomation-section">
  <div {{data_fullwidth}} class="vc_row wpb_row vc_row-fluid {% if section.settings.hide_background_mobile != blank %}mobile-bg-img-hidden{% endif %} vc_custom_{{section.id}} {% if section.settings.section_layout == '3' %}vc_row-no-padding{% endif %}"  style="margin-top: {{section.settings.margin_top | remove: 'px' | append: 'px' }};margin-bottom: {{section.settings.margin_bottom | remove: 'px' | append: 'px' }};padding: {{section.settings.padding}};">
    {{section.settings.background_gradient_color}}
    <div class="wpb_column vc_column_container vc_col-sm-12">
      <div class="vc_column-inner">
        <div class="wpb_wrapper">
          {%- if section.blocks.size > 0 -%} 
            <div class="vc_row wpb_row vc_row-fluid vc_custom_{{section.id}}8 vc_row-o-content-top vc_row-flex">
              {%- for block in section.blocks -%} 
              {%- case block.type -%}
              {%- when 'infoboxgroup' -%}
              <div class="vc_col-has-fill vc_video-bg-container wpb_column vc_column_container vc_col-lg-{{block.settings.column_width_desktop}} vc_col-md-{{block.settings.column_width_tablet}} vc_col-sm-{{block.settings.column_width_tablet}} vc_col-xs-{{block.settings.column_width_mobile}}{% if block.settings.column_hidden_tablet != blank %} vc_hidden-md{% endif %}{% if block.settings.column_hidden_mobile != blank %} vc_hidden-xs{% endif%} woodmart-parallax{%if block.settings.background_position != 'default' %} woodmart-bg-{{block.settings.background_position | replace: '_', ' ' }}{% endif %} text-{{block.settings.column_text_alignment}}">
                <div class="vc_column-inner vc_box_custom_{{ block.id }}7">
                  <div class="wpb_wrapper"> 
                    <div id="sp-{{block.id}}" class="title-wrapper woodmart-title-color-{{block.settings.heading_color}} woodmart-title-style-{{block.settings.heading_style}} woodmart-title-size-{{block.settings.heading_size}} woodmart-title-width-{{block.settings.heading_width}} text-{{block.settings.heading_postion}}" style="margin-bottom: 60px;">
                      {%- if block.settings.heading_top != '' -%}
                      <div class="title-subtitle font-default style-{{block.settings.heading_style}}">
                        {%- if settings.language_enable -%}
                        <span class="lang1">{{ block.settings.heading_top | split: '|' | first }}</span>
                        <span class="lang2">{{ block.settings.heading_top | split: '|' | last }}</span>
                        {%- else -%}
                        {{ block.settings.heading_top | split: '|' | first }}
                        {%- endif -%}
                      </div>
                      {%- endif -%} 
                      {%- if block.settings.title != '' -%}
                      <div class="liner-continer">
                        <span class="left-line"></span>
                        <h4 class="woodmart-title-container title  woodmart-font-weight-">
                          {%- if settings.language_enable -%}
                          <span class="lang1">{{ block.settings.title | split: '|' | first }}</span>
                          <span class="lang2">{{ block.settings.title | split: '|' | last }}</span>
                          {%- else -%}
                          {{ block.section.title | split: '|' | first }}
                          {%- endif -%}
                        </h4>
                        <span class="right-line"></span>
                      </div>
                      {%- endif -%} 
                      {%- if section.settings.heading_content != '' -%}
                      <div class="title-after_title">
                        {%- if settings.language_enable -%}
                        <span class="lang1">{{ section.settings.heading_content | split: '|' | first }}</span>
                        <span class="lang2">{{ section.settings.heading_content | split: '|' | last }}</span>
                        {%- else -%}
                        {{ section.settings.heading_content | split: '|' | first }}
                        {%- endif -%}
                      </div>
                      {%- endif -%}
                      <style> 
                        {%- if block.settings.heading_top == '' and block.settings.title == '' and block.settings.heading_content == '' -%}
                        .vc_custom_{{block.id}}7 .title-wrapper {
                          margin-bottom: 0;
                        }
                        {%- endif -%} 
                        {%- if block.settings.heading_size == 'custom' and block.settings.heading_custom_size != blank -%}
                        #sp-{{block.id}} .woodmart-title-container {
                          font-size: {{block.settings.heading_custom_size | remove: 'px' | append: 'px' }};
                          line-height: {{block.settings.heading_custom_size | remove: 'px' | plus: 10 | append: 'px' }};
                        }
                        {%- endif-%}
                        {%- if block.settings.heading_size == 'custom' and block.settings.heading_custom_size_tablet != blank -%}
                        @media(max-width: 1024px){
                          #sp-{{block.id}} .woodmart-title-container {
                            font-size: {{block.settings.heading_custom_size_tablet | remove: 'px' | append: 'px' }};
                            line-height: {{block.settings.heading_custom_size_tablet | remove: 'px' | plus: 10 | append: 'px' }};
                          }
                        } 
                        {%- endif-%}
                        {%- if block.settings.heading_size == 'custom' and block.settings.heading_custom_size_phone != blank -%}
                        @media(max-width: 767px){
                          #sp-{{block.id}} .woodmart-title-container {
                            font-size: {{block.settings.heading_custom_size_phone | remove: 'px' | append: 'px' }};
                            line-height: {{block.settings.heading_custom_size_phone | remove: 'px' | plus: 10 | append: 'px' }};
                          }
                        } 
                        {%- endif-%}
                        {%- if block.settings.heading_size == 'custom' and block.settings.heading_top_custom_size != blank -%}
                        #sp-{{block.id}} .title-subtitle {
                          font-size: {{block.settings.heading_top_custom_size | remove: 'px' | append: 'px' }};
                          line-height: {{block.settings.heading_top_custom_size | remove: 'px' | plus: 10 | append: 'px' }};
                        }
                        {%- endif-%}
                        {%- if block.settings.heading_size == 'custom' and block.settings.heading_top_custom_size_tablet != blank -%}
                        @media(max-width: 1024px){
                          #sp-{{block.id}} .title-subtitle {
                            font-size: {{block.settings.heading_top_custom_size_tablet | remove: 'px' | append: 'px' }};
                            line-height: {{block.settings.heading_top_custom_size_tablet | remove: 'px' | plus: 10 | append: 'px' }};
                          }
                        } 
                        {%- endif-%}
                        {%- if block.settings.heading_size == 'custom' and block.settings.heading_top_custom_size_phone != blank -%}
                        @media(max-width: 767px){
                          #sp-{{block.id}} .title-subtitle {
                            font-size: {{block.settings.heading_top_custom_size_phone | remove: 'px' | append: 'px' }};
                            line-height: {{block.settings.heading_top_custom_size_phone | remove: 'px' | plus: 10 | append: 'px' }};
                          }
                        } 
                        {%- endif-%}
                      </style>
                    </div> 
                    <div class="vc_row wpb_row vc_inner vc_row-fluid vc_row-o-content-top vc_row-flex">
                      {%- for i in (1..6) -%}
                      {%- assign icon_type = 'icon_type_' | append:i -%}
                      {%- assign icon_text = 'icon_text_' | append:i -%}
                      {%- assign icon_text_color = 'icon_text_color_' | append:i -%} 
                      {%- assign icon_svg = 'icon_svg_code_' | append:i -%}
                      {%- assign icon_image = 'icon_image' | append:i -%}
                      {%- assign title = 'title_' | append:i -%}
                      {%- assign content = 'content_' | append:i -%} 
