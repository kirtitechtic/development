<div class="container">
  <div class="wrapp-header" style="min-height: {{settings.header_min_height | remove: 'px' | append: 'px'}};">
    <div class="header-left-side">
      <div class="woodmart-burger-icon mobile-nav-icon">
        <span class="woodmart-burger"></span>
        <span class="woodmart-burger-label" {% if settings.language_enable %}data-translate="header.settings.menu"{% endif %}>{{ 'header.settings.menu' | t }}</span>
      </div>
    </div>
    <div class="site-logo">
      <div class="woodmart-logo-wrap"> 
        {%- include 'header-logo' -%}
      </div>
    </div>
    <div class="widgetarea-head">
      <div class="search-extended"> 
        <form role="search" method="get" class="searchform{% if settings.header_search_background != blank %} search-style-with-bg{% endif %} {% if settings.header_search_colletion != blank %}has-categories-dropdown{% endif %} {% if settings.header_ajax_search != blank %}woodmart-ajax-search{% endif %}" action="/search" data-thumbnail="1" data-price="1" data-post-type="product" data-count="15">
          <div>
            <label class="screen-reader-text" for="q"></label>
            <input type="text" class="s" {% if settings.language_enable %}data-translate="general.search.placeholder"{% endif %} placeholder="{{ 'general.search.placeholder' | t }}" value="" name="q" autocomplete="off" style="padding-right: 65px;">
            <input type="hidden" name="type" value="product">
            {%- if settings.header_search_colletion != blank -%}
            {%- section 'search-by-category' -%} 
            {%- endif -%}
            <button type="submit" class="searchsubmit" {% if settings.language_enable %}data-translate="general.search.submit"{% endif %}>{{ 'general.search.submit' | t }}</button>
          </div>
        </form>
        <div class="search-info-text"><span {% if settings.language_enable %}data-translate="general.search.info_search"{% endif %}>{{ 'general.search.info_search' | t }}</span></div>
        <div class="search-results-wrapper">
          <div class="woodmart-scroll has-scrollbar">
            <div class="woodmart-search-results woodmart-scroll-content" tabindex="0">
              <div class="autocomplete-suggestions" style="position: absolute; max-height: 300px; z-index: 9999; width: 308px; display: flex;"></div>
            </div>
          </div>
          <div class="woodmart-search-loader"></div>
        </div>
      </div> 
    </div> 
    <div class="right-column">
      {%- if settings.header_login != blank -%}
      <div class="woodmart-header-links">
        <ul>
          {%- if customer -%}
          <li class="item-level-0 my-account-with-text menu-item-register">
            <a href="/account" class="link" style="padding-left: 10px;"><span {% if settings.language_enable %}data-translate="header.settings.my_account"{% endif %}><img src="https://cdn.shopify.com/s/files/1/1393/2073/files/Group_3_b3e3c89e-1d45-4e83-8a41-4e8b42307419.svg?v=1634122510"></span></a> /
            <a href="/account/logout" class="link" style="padding-right: 10px;"><span {% if settings.language_enable %}data-translate="header.settings.logout"{% endif %}><img src="https://cdn.shopify.com/s/files/1/1393/2073/files/logout.svg?v=1638599061"></span></a>
          </li>  
          {%- else -%}
          <li class="item-level-0 my-account-with-text menu-item-register">
            <a href="/account/login"><span {% if settings.language_enable %}data-translate="header.settings.register"{% endif %}><img src="https://cdn.shopify.com/s/files/1/1393/2073/files/Group_3_b3e3c89e-1d45-4e83-8a41-4e8b42307419.svg?v=1634122510"></span></a>
          </li>
          {%- endif -%}
        </ul>
      </div> 
      {%- endif -%}
      {%- if settings.header_wishlist != blank and settings.collection_wishlist_dis == blank-%}
      <div class="woodmart-wishlist-info-widget">
        <a href="#">
          <span class="wishlist-info-wrap">
            <span class="wishlist-label" {% if settings.language_enable %}data-translate="header.settings.wishlist"{% endif %}>{{ 'header.settings.wishlist' | t }}</span> 
            <span class="wishlist-icon">
              <span class="count">0</span>
            </span>
          </span>
        </a>
      </div>
      {%- endif -%}
      {% include 'header-cart' %}
    </div>
  </div>
</div>
<div class="navigation-wrap navigation-color-scheme-{{settings.header_menu_text_color}} {% if settings.header_menu_bg_color != '' %}has-bg{% endif %}">
  <div class="container">
    <div class="navigations-inner">
      <div class="vertical-navigation header-categories-nav {% if template contains 'index' and settings.header_categories_menu_opened != blank %}opened-menu{% else %}show-on-hover{% endif %}" role="navigation">
        <span class="menu-opener color-scheme-{{settings.header_categories_menu_color}} has-bg">
          <span class="menu-open-label">
            <span class="burger-icon"></span>
            {% if settings.language_enable %}
            <span class="lang1">{{ settings.header_category_text | split: '|' | first }}</span>
            <span class="lang2">{{ settings.header_category_text | split: '|' | last }}</span>
            {% else %}
            {{ settings.header_category_text | split: '|' | first }}
            {% endif %}
          </span>
          <span class="arrow-opener"></span>
        </span>
        <div class="categories-menu-dropdown woodmart-navigation">
          <div class="menu-categories-container">
            {% section 'categories-menu' %}
          </div>
        </div>
      </div> 
      <div class="site-navigation woodmart-navigation menu-{{settings.header_menu_align}} navigation-style-{{settings.header_menu_style}} main-nav">
        <div class="menu-main-navigation-container">
          {% section 'main-menu' %}
        </div>
      </div>  
      {%- if settings.header_second_text1 != '' and settings.header_second_text2 != '' -%}
      <div class="menu-text-widget">
        <ul class="inline-list inline-list-with-border main-nav-style">
          {%- if settings.header_second_text1 != '' -%}
          <li>
            <a class="color-primary" href="{{settings.header_second_menu1}}">
              {% if settings.language_enable %}
              <span class="lang1">{{ settings.header_second_text1 | split: '|' | first }}</span>
              <span class="lang2">{{ settings.header_second_text1 | split: '|' | last }}</span>
              {% else %}
              {{ settings.header_second_text1 | split: '|' | first }}
              {% endif %}
            </a>
          </li>
          {%- endif -%}
          {%- if settings.header_second_text2 != '' -%}
          <li>
            <a href="{{settings.header_second_menu2}}" rel="noopener">
              {% if settings.language_enable %}
              <span class="lang1">{{ settings.header_second_text2 | split: '|' | first }}</span>
              <span class="lang2">{{ settings.header_second_text2 | split: '|' | last }}</span>
              {% else %}
              {{ settings.header_second_text2 | split: '|' | first }}
              {% endif %}
            </a>
          </li>
          {%- endif -%}
        </ul>
      </div>
      {%- endif -%}
    </div>
  </div>
</div>