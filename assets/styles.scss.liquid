  /* start state */
  .quick-view, .block-subscribe {
    opacity: 0;
    transition: all 0.4s; 
    transform: translateY(-100px);
  } 
  &.mfp-bg {
    opacity: 0;
    transition: all 0.4s;
  } 
  /* animate in */
  &.mfp-ready {
    .quick-view, .block-subscribe {
      opacity: 1;
      transform: translateY(0);
    }
    &.mfp-bg {
      opacity: 0.4;
    }
  } 
  /* animate out */
  &.mfp-removing { 
