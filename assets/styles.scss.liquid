.woodmart-shop-loader {
  display: none;
  &:not(.hidden-loader) {
    position: fixed;
    top: 50%;
    left: 50%;
    margin-top: -15px;
  }
  &.hidden-from-top {
    position: relative;
    &:after {
      position: absolute;
      left: 50%;
      margin-left: -15px;
      top: 30px;
    }
    &.hidden-loader {
      margin-left: 0!important;
    }
  }
  &.hidden-from-bottom {
    position: absolute;
    left: 50%;
    margin-left: -15px;
    bottom: 115px;
    &.hidden-loader {
      margin-left: -15px!important;
    }
  }
}  
.woodmart-loading {
  .woodmart-shop-loader {
    display: block;
    -webkit-animation: fadeIn 1s ease;
    animation: fadeIn 1s ease;
    &.hidden-loader {
      -webkit-animation: fadeIn 1s ease;
      animation: fadeIn 1s ease;
    }
    &:after {
      animation: load-spin 450ms infinite linear;
      -webkit-animation: load-spin 450ms infinite linear;
    }
  }
  .products {
    opacity: 0;
    pointer-events: none;
    transform: translateY(30px);
    -webkit-transform: translateY(30px);
  }
