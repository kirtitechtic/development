// basic font
body {
  color: #777;
  font-size: {{settings.body_font_size}}px;
  background-color: {{settings.site_background_color}};
  {%- if settings.site_background_image != blank -%}
  background-image: url({{ settings.site_background_image | img_url: 'master' }});
  {%- endif -%}
  background-repeat: {{settings.site_background_image_repeat}};
  background-size: {{settings.site_background_size}};
  background-attachment: {{settings.site_background_attachment}};
  background-position: {{settings.site_background_position | replace: '_', ' ' }}; 
}  
.group_table td.label a, 
.product-grid-item .product-title a, 
.product-title a, .widget_recent_comments ul li>a, 
.widget_recent_entries ul li a, 
.widget_recent_reviews li a, 
.woodmart-recent-posts .entry-title a:hover, 
a.product-title, 
td.product-name a {
  color: {{settings.color_product_title}};
}
.product-labels .product-label.onsale {
  background-color: {{settings.sale_label_background_color}}; 
} 
